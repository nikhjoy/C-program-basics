#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int limit,i,values[100],sum=0;
	printf("enter the limit");
	scanf("%d",&limit);
	printf("enter the values");
	for(i=0;i<limit;i++){
		scanf("%d",&values[i]);
	}
	for(i=0;i<limit;i++){
		sum=sum+values[i];
	}

	print("sum is: %d",sum);
	return EXIT_SUCCESS;
}
