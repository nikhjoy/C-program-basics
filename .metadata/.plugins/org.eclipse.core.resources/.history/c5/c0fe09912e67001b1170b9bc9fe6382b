#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,a[5];
	printf("enter values");
	for(i=0;i<5;i++){
		scanf("%d",&a[i]);
	}
	printf("entered values are : %d", a[0]);
	return EXIT_SUCCESS;
}
