#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i;
	int a[5];
	setbuf(stdout,NULL);
	printf("enter values");
	for(i=0;i<5;i++){
		scanf("%d",&a[i]);
	}
	for(i=0;i<5;i++){
		printf("entered values are : %d", a[0]);
	}

	return EXIT_SUCCESS;
}
